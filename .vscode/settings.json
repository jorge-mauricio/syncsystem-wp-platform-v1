{
  "scss.lint.unknownAtRules": "ignore",
  "postcss.validate": false,
  "css.validate": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.rulers": [100],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#f8f9fa",
    "activityBar.background": "#f8f9fa",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#bda2af",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#f8f9fa",
    "statusBar.background": "#d9dde6",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#bac1d2",
    "statusBarItem.remoteBackground": "#d9dde6",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#d9dde6",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#d9dde699",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#D9DDE6"
}
